#include <stdio.h>

int main() {
    int count;
    int i=1;
    char name[50];
    
    printf("Enter total number of tokens: \n");
    scanf("%d", &count);
    while (i <= count) {
        printf("Enter name: \n");
        scanf("%49s", name);
        printf("%s:%d\n", name, i);
        i++;
    }

    return 0;
}
